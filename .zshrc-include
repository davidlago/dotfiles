# To include, add this to ~/.zshrc:
#
# source ~/dotfiles/.zshrc-include

# Git aliases
alias gs="git status"
alias ga="git add -A"
alias gc="git commit -m"
alias gb="git branch"
alias gco="git checkout"
alias glg="git lg"

# LS colors
export CLICOLOR=1
export LSCOLORS=Dxfxcxdxbxegedabagacad
export LS_COLORS='di=93:fi=0:ln=95:pi=5:so=5:bd=5:cd=5:or=31:mi=0:ex=31:*.rpm=90'
